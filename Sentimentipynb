    {
      "cell_type": "code",
      "source": [
        "scores= []\n",
        "values = []\n",
        "\n",
        "for tweet in df['text'].to_list():\n",
        "  sentence = flair.data.Sentence(tweet)\n",
        "  sentence_model.predict(sentence)\n",
        "\n",
        "  scores.append(sentence.labels[0].score)\n",
        "  values.append(sentence.labels[0].value)\n",
        "\n",
        "df['scores'] = scores\n",
        "df['values'] = values\n",
        "\n",
        "df\n",
        "\n",
        "\n"
